<p id="notice"><%= notice %></p>

<p>
  <strong>Id:</strong>
  <%= @image.id %>
</p>

<p>
  <strong>Semester:</strong>
  <%= @student.semester %>
</p>

<p>
  <strong>Year:</strong>
  <%= @student.year %>
</p>

<p>
  <strong>Email:</strong>
  <%= @student.email %>
</p>


<%= (0...@image.student_images.count).each do |image| %>
  <div style="width:300px; height:300px;">
    <%= image_tag(@image.student_images[image], style: 'width:100%; height: auto')%>
<%end%>

<%= link_to 'Edit', edit_image_path(@image) %> |
<%= link_to 'Back', images_path %>
